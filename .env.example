# SmartPneu Product Manager - Environment Variables
# Copy this file to .env and fill in your values

# Your Shopify store name (the part before .myshopify.com)
SHOPIFY_STORE=rqv0xq-1n

# Your Admin API Access Token (from your custom app)
# This starts with shpat_
SHOPIFY_ACCESS_TOKEN=shpat_your_token_here


# Flask secret key (generate a random string for production)
SECRET_KEY=smartpneu-secret-2024
# Example: SECRET_KEY=$(openssl rand -hex 16)

PRINTER_NAME=HP_Color_LaserJet_MFP_M179fnw
AUTO_PRINT_LABELS=false